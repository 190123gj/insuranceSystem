<!DOCTYPE html>
<html lang="zh-cmn-Hans">
<head>
    #parse("/layout/common/head.vm")
    <link type="text/css" href="/styles/tmbp/project1.css" rel="stylesheet">
    <title>团体客户</title>
</head>
<body>

<div class="ui-breadcrumb">
    &nbsp;资源管理&nbsp;&gt;&nbsp;保险公司管理&nbsp;&gt;&nbsp;保险公司列表
</div>

<form class="m-main border ui-bg-fff" action="/insurance/insuranceCompany/list.htm" id="fnListSearchForm" method="POST">


    <div class="m-search fn-clear">

        <div class="item">
            保险公司名称：
            <input class="ui-text fn-w90" type="text" name="customerName" id="customerName" value="$!{querykOrder.customerName}">
        </div>
        <div class="item">
            单位性质：
            <input class="fn-validate" type="radio" name="companyNature"  value=""  id="kind1"   #if($!{querykOrder.companyNature}=="") checked #end/> <label for="kind1">全选</label>
            <input class="fn-validate" type="radio" name="companyNature"  value="head"  id="kind1"   #if($!{querykOrder.companyNature}=="head") checked #end/> <label for="kind1">总公司</label>
            <input class="fn-validate" type="radio" name="companyNature"  value="branch"  id="kind2"   #if($!{querykOrder.companyNature}=="branch") checked #end/>  <label for="kind2">分支机构</label>
        </div>
        <div class="item">
            联系人：
            <input class="ui-text fn-w90" type="text" name="contactMan" id="contactMan" value="$!{querykOrder.contactMan}">
        </div>
        <div class="item">
            联系电话：
            <input class="ui-text fn-w90" type="text" name="contactMobile" id="contactMobile" value="$!{querykOrder.contactMobile}">
        </div>

        <div class="item">
       &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;简称：
            <input class="ui-text fn-w90" type="text" name="abbr" id="abbr" value="$!{querykOrder.abbr}">
        </div>

##        <div class="item">
##            团体类型：
##            <select  name="certType" id="certType" class="ui-select fn-w200 fnChangeInput">
##                <option >全部</option>
##                #foreach($certType in $!{certTypeList})
##                    <option value="$!{certType.code}" #if( "$!{querykOrder.certType.code}"=="$!{certType.code}") selected="selected" #end >$!{certType.message}</option>
##                #end
##            </select>
##        </div>
##        <div class="item">
##            证件号码：
##            <input class="ui-text fn-w90" type="text" name="certNo" id="certNo" value="$!{querykOrder.certNo}">
##        </div>
        <div class="item">
            <a class="ui-btn ui-btn-fill ui-btn-seek" id="fnListSearchBtn">查询</a>
        </div>
    </div>


    <div class="fn-pl15 fn-pr15">

        <div class="fn-mb20 fn-mt20">

            <a class="ui-btn ui-btn-fill ui-btn-green-empty" href="/insurance/insuranceCompany/add.htm"><b>+</b>新增保险公司&nbsp;</a>

        </div>


        <table class="m-table m-table-list fn-tac" id="list">

            <thead>
            <tr id="fnListSearchTh">
                <th>序号</th>
                <th>团体名称</th>
                <th>证件类型</th>
                <th>证件号码</th>
                <th>联系人</th>
                <th>联系电话</th>
                <th>创建时间</th>
                <th>修改时间</th>
                <th width="200px">操作</th>
            </tr>
            </thead>
            <tbody>
                #foreach($item in $page.result)
                <tr>
                    <td>	$!{velocityCount} </td>
                    <td> <a href="/insurance/insuranceCompany/info.htm?userId=$!{item.userId}">	$!{item.customerName} </a></td>
                    <td>	$!{item.certType.message} </td>
                    <td>	$!{item.certNo} </td>
                    <td>	$!{item.contactMan} </td>
                    <td>	$!{item.contactMobile} </td>
                    <td>	$!dateUtil.simpleFormat($!{item.rawAddTime}) </td>
                    <td>	$!dateUtil.simpleFormat($!{item.rawUpdateTime}) </td>
                    <td>
                        <a href="/insurance/insuranceCompany/edit.htm?userId=$!{item.userId}">[ 编辑 ]</a><br/>
                        <a href="/insurance/insuranceCompany/info.htm?userId=$!{item.userId}">[ 查看详情 ]</a><br/>
                        <a href="/insurance/insuranceProtocol/list.htm?protocolUserId=$!{item.userId}">[ 协议清单 ]</a><br/>

                    </td>
                </tr>
                #end
            </tbody>
        </table>



        #parse("/layout/common/pages.vm")


    </div>

</form>



    #parse("/layout/common/footer.vm")

    #parse("/layout/common/commonjs.vm")

<script type="text/javascript">
    seajs.use($_GLOBAL.zyw+'/customer/customerPerson');
</script>
</body>
</html>