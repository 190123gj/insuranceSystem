		                     #else
		                     <a href="javascript:void(0);" class="renewalPayment " >[ 缴费信息维护 ]</a>
		                     <input type="hidden" class="renewalPaymentVal #if ($!item.paymentStatus == 0) newAddVal #end"" name="insuranceBillRenewal">
		                     #end
