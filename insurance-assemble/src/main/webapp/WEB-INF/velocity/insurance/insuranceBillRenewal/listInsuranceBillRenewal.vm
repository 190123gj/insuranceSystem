	<form id="form" action="/insurance/insuranceBillRenewal/insuranceBillRenewal.json" callbackhref="/insurance/insuranceBillRenewal/list.htm?businessBillId=$!{info.businessBillId}">
		<input type="hidden" name="businessBillId" value="$!{info.businessBillId}" />
        <div class="fn-pl15 fn-wp80 m-item itemBox diyNameBox">
        	<div>
            <table class="m-table m-table-list itemsList renvalwalBillTable">

                <thead>
                <tr id="fnListSearchTh">
                    <th>序号</th>
                    <th>保单年度</th>
                    <th>单据号</th>
                    <th>缴费期数</th>
                    <th>应收日期</th>
                    <th>保险费</th>
                    <th>实收日期</th>
                    <th>录入时间</th>
                    <th width="200px">操作</th>
                </tr>
               </thead>
               	   #set($index = 0)
                    #foreach ($list in $!{planList})
                    #set($a = 0)
                    #set($index = $index + 1)
                        #foreach ($listDetail in $list.list)
                             #set($a = $a+1)
                  		  <tr diyname="businessBillPayPlanOrders" year="$!list.year" businessBillId="$!{listDetail.businessBillId}" period="$!{listDetail.period}">
	                  		  #if($a == 1) 
	                  		    <td class="fn-tac"  rowspan ="$!{list.length}">$index</td>
	                  		    <td class="fn-tac year" rowspan ="$!{list.length}">$!list.year</td>
	                  		  #else 
	                  		     <td class="fn-tac fn-hide">$index</td>
	                  		     <td class="fn-tac year fn-hide">$!list.year</td>
	                  		  #end
	                  	   <input type="hidden" name="serial" value="$a">
	                  	   <input type="hidden" name="currentYear" value="$index">
                           <input type="hidden" name="year" value="$!list.year">
                           <input type="hidden" name="appserialPeriod" value="$listDetail.period">
                           <input type="hidden" name="premiumAmount" value="$!listDetail.premiumAmount.amount">
                           <input type="hidden" name="planPayDate" value="$!dateUtil.formatDay($listDetail.planPayDate)">
                           <input type="hidden" name="paymentStatus" value="$listDetail.paymentStatus" class="paymentStatus">
                           <input type="hidden" name="payPlanId" value="$listDetail.payPlanId">
                            <td class="fn-tac">$!{listDetail.billNo}</td>
                            <td class="fn-tac appserialPeriod">$listDetail.period</td>
                            <td class="fn-tac planPayDate">$!dateUtil.formatDay($!listDetail.planPayDate)</td>
                            <td class="fn-tac">$!listDetail.premiumAmount.amount</td>
                            #if($listDetail.paymentStatus > 0)
                              <td class="fn-tac"><input class="text fn-tac" value="$!dateUtil.formatDay($!{listDetail.actualPayDate})"" readOnly  type="text"  name="actualPayDate"/></td>
                            #else
                            <td class="fn-tac"><input class="text laydate-icon fn-tac actualPayDate lifeActualPayDate index_$n" value="$!dateUtil.formatDay($!{listDetail.actualPayDate})""  type="text" onclick="laydate({istime: true,format: 'YYYY-MM-DD'})" name="actualPayDate"/></td>
                            #end
                             <td class="fn-tac">$!dateUtil.formatDay($!{listDetail.rowAddTime})</td>
                            <td class="fn-tac">
