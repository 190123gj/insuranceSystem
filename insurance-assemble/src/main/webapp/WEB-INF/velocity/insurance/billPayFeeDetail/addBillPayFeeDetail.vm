<h2 class="m-h2-title">xxxx</h2>

<div class="fn-blank20"></div>

<div class="m-main border ui-bg-fff">
    
    <form id="form" action="/insurance/xxxx.json" method="POST">
			 	<div class="m-item">
            <label class="m-label"><span class="m-required">*</span>实付金额：</label>
            <input class="ui-text fn-w200 fnInput fnRequired" type="text" name="actualPayFee" id="actualPayFee" value="" maxlength="20">
        </div>
			 	<div class="m-item">
            <label class="m-label"><span class="m-required">*</span>费用性质：</label>
            <input class="ui-text fn-w200 fnInput fnRequired" type="text" name="feeProperty" id="feeProperty" value="" maxlength="20">
        </div>
			 	<div class="m-item">
            <label class="m-label"><span class="m-required">*</span>：</label>
            <input class="ui-text fn-w200 fnInput fnRequired" type="text" name="id" id="id" value="" maxlength="20">
        </div>
			 	<div class="m-item">
            <label class="m-label"><span class="m-required">*</span>：</label>
            <input class="ui-text fn-w200 fnInput fnRequired" type="text" name="rowUpdateTime" id="rowUpdateTime" value="" maxlength="20">
        </div>
			 	<div class="m-item">
            <label class="m-label"><span class="m-required">*</span>关联的保单的id：</label>
            <input class="ui-text fn-w200 fnInput fnRequired" type="text" name="businessBillId" id="businessBillId" value="" maxlength="20">
        </div>
			 	<div class="m-item">
            <label class="m-label"><span class="m-required">*</span>应付金额：</label>
            <input class="ui-text fn-w200 fnInput fnRequired" type="text" name="payFee" id="payFee" value="" maxlength="20">
        </div>
			 	<div class="m-item">
            <label class="m-label"><span class="m-required">*</span>记录新增时间：</label>
            <input class="ui-text fn-w200 fnInput fnRequired" type="text" name="rowAddTime" id="rowAddTime" value="" maxlength="20">
        </div>
			 	<div class="m-item">
            <label class="m-label"><span class="m-required">*</span>保单申请表id：</label>
            <input class="ui-text fn-w200 fnInput fnRequired" type="text" name="settlementApplyId" id="settlementApplyId" value="" maxlength="20">
        </div>
			 	<div class="m-item">
            <label class="m-label"><span class="m-required">*</span>代扣税金：</label>
            <input class="ui-text fn-w200 fnInput fnRequired" type="text" name="withholdTaxes" id="withholdTaxes" value="" maxlength="20">
        </div>
			 	<div class="m-item">
            <label class="m-label"><span class="m-required">*</span>收款人姓名：</label>
            <input class="ui-text fn-w200 fnInput fnRequired" type="text" name="receiverName" id="receiverName" value="" maxlength="20">
        </div>
		      
        

        <div class="m-blank fn-mt20 fn-mb20"></div>
        <div class="fn-tac fn-pt20 fn-pb20">
            <input class="ui-btn ui-btn-submit ui-btn-next fn-ml5 fn-mr5 submit" type="button" value="提交">
            <a href="/insurance/xxxx.htm" class="ui-btn ui-btn-submit ui-btn-back fn-ml5 fn-mr5">返回</a>
        </div>

    </form>

</div>

