<!DOCTYPE html>
<html lang="zh-cmn-Hans">
<head>
#parse("/layout/common/head.vm")
<link type="text/css" href="/styles/tmbp/project4.css" rel="stylesheet">
<title></title>
</head>
<body>

<div class="ui-breadcrumb">
    &nbsp;结算管理&nbsp;&gt;&nbsp;结算清单&nbsp;&gt$!{info.type}编辑
</div>

<div class="m-main border ui-bg-fff">
    <form id="form" action="/insurance/billSettlementApply/editSubmit.json" method="POST" callbackhref="/insurance/billSettlementApply/list.htm" process="true">
 	#parse("/layout/insurance/billSettlementApply/viewBillSettlementApply.vm")
        <div class="m-blank fn-mt20 fn-mb20"></div>
        <div class="fn-tac fn-pt20 fn-pb20">
      	    <input class="ui-btn ui-btn-submit ui-btn-next fn-ml5 fn-mr5 submitBtn" type="button" value="提交">
            <a href="/insurance/billSettlementApply/list.htm" class="ui-btn ui-btn-submit ui-btn-back fn-ml5 fn-mr5">返回</a>
        </div>

    </form>

</div>

#parse("/layout/common/footer.vm")
#parse("/layout/common/commonjs.vm")
#parse("/layout/common/commonue.vm")
<script src="/js/laydate/laydate.js"></script>
<script>
    seajs.use($_GLOBAL.zyw+'/businessBill/addBusinessBill.js');

    $(function(){

        $("body").on('blur','.shouldPayFee',function(){
           var _val = $(this).val()*1;
            var $serviceFee = $(".serviceFee");
           var $total = $("#commonVal").val()*1; //总的佣金
            if ($total-_val < 0) {
               $(this).val($total);
                    $serviceFee.val(0.00);
            } else {
                    $serviceFee.val($total-_val);
            }
            $(".individualTax").blur()
        });


        $("body").on('blur','.individualTax',function(){
            var _val = $(this).val()*1;
            var _shouldPayFee = $(".shouldPayFee").val()*1;
            if (_shouldPayFee-_val < 0) {
                $(this).val(_shouldPayFee);
                $(".actualPayFee").val(0);
            } else {
                $(".actualPayFee").val(_shouldPayFee-_val);
            }
        });



    });

</script>

</body>
</html>